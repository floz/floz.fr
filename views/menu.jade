include mixins

-var entries = [ { url: '/', id: "about", name: "ABOUT ME" }, { url: '/works', id: "works", name: "WORKS" } ]

nav#menu
	ul
		-for entry, i in entries
			-if( entry.id == navId )
				li.activated.animated(class="d" + (i + 1) )
					mixin bt( entry.name, entry.url )
			-else
				li.animated(class="d" + (i + 1) )
					mixin bt( entry.name, entry.url )
	.clear